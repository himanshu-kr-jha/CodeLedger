{
  "manifest_version": 3,
  "name": "Code Ledger",
  "version": "1.0",
  "description": "Its Chrome extension that lets you log coding problems you solve â€” directly into a Google Sheet. Helps you track and manage the coding problems you solve across various platforms",
  "permissions": [
    "activeTab",
    "scripting",
    "identity",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./images/icon.svg",
    "48": "./images/icon.svg",
    "128": "./images/icon.svg"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "1023364826088-fbo6up8c5tbt0hht07ktt69qor45jaq0.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets",
      "https://www.googleapis.com/auth/drive.file",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "icons": {
    "16": "./images/icon.svg",
    "48": "./images/icon.svg",
    "128": "./images/icon.svg"
  },
  "host_permissions": [
    "https://www.googleapis.com/*"
  ]
}